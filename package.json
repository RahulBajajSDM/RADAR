{
  "name": "Radar",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "./node_modules/.bin/eslint . --ext .js",
    "android-build": "watchman watch-del-all && cd android && ./gradlew --stop && ./gradlew clean && ./gradlew assembleRelease",
    "rp": "./node_modules/.bin/prettier --write \"**/*.json\" \"**/*.js\" \"!dist/**/*.js\"",
    "precommit": "npm run rp && lint-staged",
    "android:release": "npx react-native run-android --variant=release",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'"
  },
  "dependencies": {
    "@mauron85/react-native-background-geolocation": "^0.6.3",
    "@react-native-community/async-storage": "^1.6.1",
    "@react-native-community/datetimepicker": "^3.0.3",
    "@react-native-community/netinfo": "^5.9.7",
    "@react-native-community/push-notification-ios": "^1.7.1",
    "@react-native-firebase/app": "^8.4.6",
    "@react-native-firebase/crashlytics": "^8.4.11",
    "@react-native-firebase/messaging": "^7.9.1",
    "axios": "^0.21.0",
    "eventemitter4": "^0.2.4",
    "jwt-decode": "^3.0.0",
    "moment": "^2.29.1",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-android-location-services-dialog-box": "^2.8.2",
    "react-native-app-intro-slider": "^3.0.0",
    "react-native-bluetooth-state-manager": "^1.3.1",
    "react-native-geo-fencing": "^0.1.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.1",
    "react-native-material-textfield": "^0.16.1",
    "react-native-modal-datetime-picker": "^9.0.0",
    "react-native-modal-dropdown": "^0.7.0",
    "react-native-navigation": "^4.8.4",
    "react-native-push-notification": "^7.3.1",
    "react-native-rename": "^2.4.1",
    "react-native-render-html": "^5.1.1",
    "react-native-root-siblings": "^4.0.6",
    "react-native-splash-screen": "^3.2.0",
    "react-native-text-input-mask": "^2.0.0",
    "react-native-vector-icons": "^7.1.0",
    "react-native-webview": "^7.4.0",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "seamless-immutable": "^7.1.4",
    "socket.io-client": "^2.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@babel/runtime": "^7.5.0",
    "@react-native-community/eslint-config": "^0.0.7",
    "babel-jest": "^24.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.15.0",
    "husky": "^3.0.7",
    "jest": "^24.1.0",
    "lint-staged": "8.1.3",
    "metro-react-native-babel-preset": "^0.51.1",
    "prettier": "1.18.2",
    "react-test-renderer": "16.8.1"
  },
  "lint-staged": {
    "*.{js,json}": [
      "git add"
    ]
  },
  "jest": {
    "preset": "react-native"
  }
}
